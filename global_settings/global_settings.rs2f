// module:global_settings

meta {
    name = "Nismo Filter";
}

#define ITEM_ICONS
#define GLOBAL_DESPAWN_TIMER
#define GLOBAL_STYLING

apply (ITEM_ICONS) {
    icon = CurrentItem();
}
apply (name:"*") {
    showDespawn = true;
    GLOBAL_STYLING
}
apply (value:<GLOBAL_DESPAWN_TIMER) {
    showDespawn = false;
}
// endmodule:global_settings